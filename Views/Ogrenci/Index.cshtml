@using OgrenciNotMvc.Models.EntityFramework
@model List<TBLOGRENCILER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Burası Öğrenci Sayfası</h2>

<table class="table table-bordered">
    <tr>
        <th>ÖĞRENCİ ID</th>
        <th>ADI</th>
        <th>SOYADI</th>
        <th>CİNSİYET</th>
        <th>KULÜP</th>
    </tr>
    <tbody>
        @foreach (var ogr in Model)
        {
            <tr>
                <td>@ogr.OGRENCIID</td>
                <td>@ogr.OGRAD</td>
                <td>@ogr.OGRSOYAD</td>
                <td>@ogr.OGRCINSIYET</td>
                <td>@ogr.OGRKULUP</td>
            </tr>
        }
    </tbody>
</table>

<a href="/Ogrenci/YeniOgrenci" class=" btn btn-primary">Yeni Öğrenci Kaydı</a>